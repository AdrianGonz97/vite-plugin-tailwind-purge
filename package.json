{
	"name": "vite-plugin-tailwind-purge",
	"version": "0.0.0",
	"description": "Vite plugin for PurgeCSS",
	"module": "dist/index.mjs",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"repository": {
		"url": "https://github.com/AdrianGonz97/vite-plugin-tailwind-purge",
		"type": "git"
	},
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"lint": "tsc",
		"release": "pnpm run build && changeset publish",
		"format": "prettier --write \"./src/**/*.{ts,svelte}\""
	},
	"keywords": [
		"purgeCSS",
		"sveltekit",
		"svelte",
		"rollup",
		"tailwind",
		"vite",
		"skeleton",
		"vite-plugin"
	],
	"author": "AdrianGonz97",
	"license": "MIT",
	"dependencies": {
		"purgecss": "^5.0.0",
		"estree-walker": "^3.0.3"
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.2",
		"@types/node": "^18.11.18",
		"prettier": "^2.8.1",
		"tsup": "^6.5.0",
		"typescript": "^4.9.4",
		"vite": "^4.1.1"
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"vite": "^4.1.1"
	}
}
